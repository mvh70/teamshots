{
  "name": "teamshots",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --experimental-https -p 3000",
    "build": "npx prisma generate && next build --webpack",
    "start": "next start",
    "start:prod": "concurrently \"npm run start\" \"npm run worker\"",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:status": "prisma migrate status",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "worker": "tsx src/queue/worker.ts | tee -a logs/worker.log",
    "worker:dev": "tsx watch src/queue/worker.ts | tee -a logs/worker-dev.log",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test tests/e2e/",
    "test:e2e:ui": "playwright test tests/e2e/ --ui",
    "test:e2e:headed": "playwright test tests/e2e/ --headed",
    "test:e2e:debug": "playwright test tests/e2e/ --debug",
    "test:dev": "DATABASE_URL=postgresql://postgres:postgres@localhost:5432/teamshots_test?schema=public npm run dev",
    "test:all": "npm run test && npm run test:e2e",
    "test:selfie-flow": "playwright test tests/e2e/selfie-flow/",
    "test:selfie-flow:debug": "playwright test tests/e2e/selfie-flow/ --debug",
    "test:unit": "jest tests/unit/",
    "test:unit:watch": "jest tests/unit/ --watch",
    "test:integration": "playwright test tests/integration/",
    "test:integration:ui": "playwright test tests/integration/ --ui",
    "test:setup": "npx playwright install --with-deps",
    "test:report": "npx playwright show-report",
    "type-check": "rm -f tsconfig.tsbuildinfo && tsc --noEmit",
    "type-check:next": "next build 2>&1 | grep -E '(Type error|TS[0-9]+)' || true",
    "type-check:patterns": "node scripts/check-implicit-any.js",
    "type-check:production": "rm -rf .next tsconfig.tsbuildinfo && tsc --noEmit && npm run build",
    "pre-push": "npm run type-check",
    "validate": "npm run type-check && npm run test",
    "db:reset": "node scripts/reset-dev-db.js",
    "db:test:reset": "node tests/cleanup-test-db.js",
    "db:test:setup": "node tests/setup-test-db.js",
    "queue:clear": "tsx scripts/clear-queue.ts",
    "queue:clear:force": "tsx scripts/clear-queue.ts --force",
    "queue:clear:all": "tsx scripts/clear-queue.ts --all",
    "queue:clear:all:force": "tsx scripts/clear-queue.ts --all --force",
    "logs:tail": "./scripts/view-worker-logs.sh tail",
    "logs:dev": "./scripts/view-worker-logs.sh dev",
    "logs:last": "./scripts/view-worker-logs.sh last",
    "logs:search": "./scripts/view-worker-logs.sh search",
    "logs:clear": "./scripts/view-worker-logs.sh clear",
    "generate:clothing-previews": "tsx scripts/generate-clothing-previews.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@aws-sdk/client-s3": "^3.911.0",
    "@aws-sdk/s3-presigned-post": "^3.911.0",
    "@aws-sdk/s3-request-presigner": "^3.911.0",
    "@bull-board/api": "^6.13.1",
    "@bull-board/express": "^6.13.1",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.30.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@next/third-parties": "^16.1.4",
    "@openrouter/sdk": "^0.2.9",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.3.0",
    "@react-email/components": "^0.5.6",
    "@tensorflow-models/blazeface": "^0.1.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/papaparse": "^5.5.2",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.10.0",
    "bullmq": "^5.61.0",
    "color-name-list": "^13.25.0",
    "concurrently": "^8.2.2",
    "dompurify": "^3.3.0",
    "file-type": "^21.0.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.554.0",
    "mime-types": "^3.0.1",
    "next": "16.0.10",
    "next-auth": "^5.0.0-beta.29",
    "next-intl": "^4.8.2",
    "onborda": "^1.2.5",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "posthog-js": "^1.280.1",
    "qrcode.react": "^4.2.0",
    "react": "19.2.3",
    "react-colorful": "^5.6.1",
    "react-compare-image": "^3.5.10",
    "react-dom": "19.2.3",
    "react-email": "^4.3.0",
    "replicate": "^1.4.0",
    "resend": "^6.1.2",
    "sharp": "^0.34.4",
    "stripe": "^19.1.0",
    "swr": "^2.3.8",
    "tsx": "^4.20.6",
    "validator": "^13.15.15",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.23",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/validator": "^13.15.3",
    "@vitest/ui": "^4.0.1",
    "autoprefixer": "^10.4.23",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-gyp": "^10.0.0",
    "postcss": "^8.5.6",
    "prisma": "^7.2.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5",
    "vitest": "^4.0.1"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  },
  "optionalDependencies": {
    "@img/sharp-libvips-linux-x64": "1.2.4",
    "@img/sharp-linux-x64": "0.34.5",
    "@parcel/watcher": "^2.5.6",
    "@parcel/watcher-linux-x64-glibc": "^2.5.6"
  }
}
